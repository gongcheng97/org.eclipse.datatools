 <extension
         point="org.eclipse.datatools.connectivity.driverExtension">
         <!--     
            jarList="[com.sybase.jconnect60]\lib\jconn3.jar"
            -->
      <driverTemplate
            createDefault="false"
            emptyJarListIsOK="false"
            id="org.eclipse.datatools.enablement.sybase.asa.drivertemplate"
            jarList="jconn3.jar"
            name="%asa9.driver.template"
            parentCategory="org.eclipse.datatools.enablement.sybase.asa.9x"
            valuesProvider="org.eclipse.datatools.enablement.sybase.asa.ASADriverValuesProvider">
         <properties>
            <property
                  generated="false"
                  id="org.eclipse.datatools.connectivity.db.driverClass"
                  name="Driver Class"
                  value="com.sybase.jdbc3.jdbc.SybDriver"
                  required="true"
                  visible="true"/>
            <property
                  generated="false"
                  id="org.eclipse.datatools.connectivity.db.vendor"
                  name="Vendor"
                  value="Sybase_ASA"
                  required="true"
                  visible="false"/>
            <property
                  generated="false"
                  id="org.eclipse.datatools.connectivity.db.version"
                  name="Version"
                  value="9.x"
                  required="true"
                  visible="false"/>   
            <property
                  generated="false"
                  id="org.eclipse.datatools.connectivity.db.databaseName"
                  name="Database Name"
                  value="asademo"
                  required="true"
                  visible="true"/>                             
            <property
                  generated="false"
                  id="org.eclipse.datatools.connectivity.db.URL"
                  name="Connection URL"
                  value="jdbc:sybase:Tds:host:port/database"
                  required="true"
                  visible="false"/>
            <property
                  generated="false"
                  id="org.eclipse.datatools.connectivity.db.username"
                  name="User ID"
                  value="dba"
                  required="false"
                  visible="true"/>
            <property
                  generated="false"
                  id="org.eclipse.datatools.connectivity.db.password"
                  name="Password"
                  value="sql"
                  required="false"
                  visible="true"/>
  			<property
           id="org.eclipse.datatools.enablement.sybase.host"
